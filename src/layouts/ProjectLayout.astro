---
import type { ProjectFrontMatter } from '../utils/projects';
import ArticleLayout from './ArticleLayout.astro';

export interface Props {
	content: ProjectFrontMatter;
}

const {content} = Astro.props as Props;
---

<ArticleLayout title={content.title} description={content.description} imgSrc={content.imgSrc} imgAlt={content.imgAlt}>
	<slot slot="article"/>
</ArticleLayout>